{
  "name": "hapi-react",
  "version": "1.0.0",
  "description": "Hapi universal ES6 react with stylus and material ui",
  "main": "index.js",
  "scripts": {
    "start": "env NODE_ENV=development nodemon lib/index.js --exec babel-node",
    "watch": "env NODE_ENV=development webpack -w --config webpack.config.dev.js",
    "build:server": "babel lib -d dist",
    "build:client": "env NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "serve": "npm run build && env NODE_ENV=production nodemon dist/index.js --exec babel-node",
    "update-schema": "babel-node ./scripts/updateSchema.js"
  },
  "author": "Mattias Ewers",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-graphql": "^2.0.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-node5": "^1.1.1",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-runtime": "^6.3.19",
    "cross-env": "^1.0.7",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "good": "^6.4.0",
    "good-console": "^5.2.0",
    "good-file": "^5.1.1",
    "good-http": "^5.1.0",
    "hapi-webpack": "^1.0.0",
    "postcss-loader": "^0.8.0",
    "poststylus": "^0.2.2",
    "rimraf": "^2.5.0",
    "rucksack-css": "^0.8.5",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.4.2",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "@risingstack/graffiti": "^2.1.2",
    "@risingstack/graffiti-mongoose": "^5.0.3",
    "babel-polyfill": "^6.3.14",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "ejs": "^2.3.4",
    "faker": "^3.0.1",
    "graphiql": "^0.4.5",
    "graphql": "^0.4.14",
    "handlebars": "^4.0.5",
    "hapi": "^12.1.0",
    "hapi-graphql": "^0.4.0",
    "hapi-react-views": "^5.0.2",
    "history": "^2.0.0-rc2",
    "inert": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-relay": "^0.4.2",
    "kerberos": "0.0.17",
    "lodash": "^4.0.0",
    "lokka": "^1.6.1",
    "lokka-transport-http": "^1.3.0",
    "material-ui": "^0.14.0-rc1",
    "minimist": "^1.2.0",
    "mongoose": "^4.3.1",
    "normalize-styl": "^3.0.3",
    "pg": "^4.4.3",
    "react": "^0.14.6",
    "react-doc-meta": "^0.2.0",
    "react-document-meta": "^2.0.0-rc2",
    "react-dom": "^0.14.6",
